rm /var/lib/pacman/sync*
pacman-key init
#gpg --refresh-keys
pacman-key --populate archlinux manjaro biglinux
pacman-key --refresh-keys
yes y | LANG=C pacman -Syy archlinux-keyring manjaro-keyring

#Add BigLinux key
pacman-key --keyserver hkps://keys.openpgp.org -r 0x45EC44879815E2A6
pacman-key --lsign-key 0x45EC44879815E2A6
    
    ;;


    *)
	echo " Usage:
	--fix-keys This script can be used to reinstall system keys, but only BigLinux, ArchLinux and Manjaro keys will be reinstalled, if you added more repositories the keys will be deleted and you need to add them again.

	--upgrade-now Forcing the update might remove some programs, but if the regular update isn't working, this might be a good solution. Remember that if you are using BTRFS partition with snapshots, if the forced update brings problems, you can restore a snapshot."
	exit
    ;;

